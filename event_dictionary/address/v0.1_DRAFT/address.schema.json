{
  "$id": "https://ons.gov.uk/address.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Address",
  "type": "object",
  "properties": {
    "address": {
      "type": "object",
      "properties": {
        "version": {
          "description": "The version of the schema that this address conforms to.",
          "type": "string",
          "examples": ["v0.1_DRAFT"]
        },
        "addressLine1": {
          "description": "The first line of the address.",
          "type": "string",
          "examples": ["Flat 2A, Greenslade Tower"]
        },
        "addressLine2": {
          "description": "The second line of the address.",
          "type": "string",
          "examples": ["123 The Avenue"]
        },
        "addressLine3": {
          "description": "The third line of the address.",
          "type": "string",
          "examples": ["Camden"]
        },
        "townName": {
          "description": "The name of the town or city.",
          "type": "string",
          "examples": ["London"]
        },
        "postcode": {
          "description": "The postcode.",
          "type": "string",
          "examples": ["NW5 1XX"]
        },
        "uprn": {
          "description": "Unique Property Reference Number (UPRN).",
          "type": "string",
          "examples": ["99999999999"]
        }
      },
      "required": ["version", "addressLine1", "townName", "postcode", "uprn"],
      "additionalProperties": false
    }
  },
  "required": ["address"],
  "additionalProperties": false
}
